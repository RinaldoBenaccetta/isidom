{
  "name": "isi",
  "version": "1.0.0",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.1.1",
    "@types/jest": "^25.1.1",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^25.1.0",
    "jest-cli": "^24.9.0",
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.1",
    "rollup": "^1.27.0",
    "ts-jest": "^25.1.0",
    "typescript": "^3.7.5"
  },
  "description": "A browser side Javascript library, for helping to easily and intuitively manipulate the DOM with minification efficiency of bundled apps.",
  "browser": "src/index.js",
  "keywords": [
    "DOM",
    "manipulation",
    "browser",
    "class",
    "id",
    "selector",
    "attribute",
    "value",
    "data",
    "style",
    "text",
    "sibling"
  ],
  "directories": {
    "lib": "src/lib",
    "test": "src/test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RinaldoBenaccetta/isi.git"
  },
  "scripts": {
    "test": "jest",
    "testwatch": "jest --watchAll",
    "build": "npx rollup --config",
    "doc": "documentation build --config documentation.yml build src/lib/** -f md -o readme.md --markdown-toc-max-depth 3",
    "docwatch": "documentation build --config documentation.yml build src/lib/** -f md -o readme.md -w --markdown-toc-max-depth 3",
    "doclint": "documentation lint src/lib/**"
  },
  "author": "Rinaldo Benaccetta",
  "license": "MIT",
  "dependencies": {}
}
