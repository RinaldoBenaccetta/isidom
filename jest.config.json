{
  "verbose": false,
  "preset": "ts-jest/presets/js-with-ts",
  "testEnvironment": "jsdom",
  "setupFilesAfterEnv": ["./jest.setup.js"],
  "collectCoverage": true,
  "collectCoverageFrom": [
    "!**/*.experiment.js",
    "**/*.js",
    "!**/test/fixtures/**/*.js",
    "!**/test/coverage/**/*.js",
    "!/node_modules/",
    "!**/*.config.js",
    "!**/dist/**/*.js"
  ],
  "coverageDirectory": "src/test/coverage",
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"]
}
